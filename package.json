{
  "name": "cfs-plugins",
  "publisher": "analogdevices",
  "description": "Plugins for CodeFusion Studio",
  "author": "Analog Devices, Inc.",
  "version": "2.0.1",
  "private": true,
  "scripts": {
    "build": "yarn ws:api build && yarn ws:plugins build",
    "clean": "yarn ws:api clean && yarn ws:plugins clean",
    "clean-build": "yarn clean && yarn build",
    "genai": "yarn ws:plugins genai",
    "ws:api": "yarn workspace cfs-plugins-api",
    "ws:plugins": "yarn workspace plugins",
    "test": "mocha --require ts-node/register --loader ts-node/esm --recursive tests/**/*.test.ts"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@tsconfig/node18": "^18.2.4",
    "@tsconfig/node20": "^20.1.6",
    "@types/chai": "^5",
    "@types/chai-as-promised": "^8",
    "@types/mocha": "^10.0.10",
    "@types/node": "20",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "chai": "^5.1.2",
    "chai-as-promised": "^8.0.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^10.1.5",
    "mocha": "^11.1.0",
    "rollup": "^4.34.8",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-typescript-resolve": "^1.0.1",
    "shx": "^0.3.4",
    "sinon": "^19.0.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3"
  },
  "type": "module",
  "license": "Apache-2.0",
  "workspaces": [
    "api",
    "plugins"
  ],
  "packageManager": "yarn@4.2.2"
