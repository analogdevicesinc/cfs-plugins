{
  "Copyright": "Copyright (c) <%= new Date().getFullYear() %> Analog Devices, Inc.  All rights reserved. This software is proprietary to Analog Devices, Inc. and its licensors.",
  "DataModelVersion": "<%= it.dataModelVersion || '1.0.0' %>",
  "DataModelSchemaVersion": "<%= it.dataModelSchemaVersion || '1.0.0' %>",
  "Soc": "<%= it.soc %>",
  "Package": "<%= it.package %>",
  "BoardName": "<%= it.board %>",
  "Timestamp": "<%= new Date().toISOString() %>",
  "Projects": [
<% if (it.projects && it.projects.length) { %>
  <% it.projects.forEach((project, index) => { %>
  {
      "CoreId": "<%= project.coreId %>",
      "ProjectId": "<%= project.id %>",
      "PluginId": "<%= project.pluginId %>",
      "PluginVersion": "<%= project.pluginVersion %>",
      "FirmwarePlatform": "<%= project.firmwarePlatform %>",
      "ExternallyManaged": <%= !project.isEnabled %>,
      "PlatformConfig": {

<% if (project.platformConfig && Object.keys(project.platformConfig).length > 0) { %>
        <%= Object.keys(project.platformConfig)
            .map(key => `"${key}": ${JSON.stringify(project.platformConfig[key])}`)
            .join(',\n        ') %> 
<% } %>
      },
      "Partitions": [],
      "Peripherals": []
    }<% if (index < it.projects.length - 1) { %>,
<% } %>
  <% }) %>
<% } _%> <%-%>

  ],
  "Pins": [],
  "ClockNodes": []
}
