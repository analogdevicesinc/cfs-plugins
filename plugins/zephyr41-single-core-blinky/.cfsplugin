  "features": {
    "workspace": {
      "files": [
        {
          "src": "files/.vscode/launch.json",
          "dst": "${context.soc.toLowerCase() === 'max32657' ? 'm33' : 'm4'}/.vscode/launch.json"
        },
        {
          "src": "files/.vscode/c_cpp_properties.json",
          "dst": "${context.soc.toLowerCase() === 'max32657' ? 'm33' : 'm4'}/.vscode/c_cpp_properties.json"
        },
        {
          "src": "files/src/main.c",
          "dst": "${context.soc.toLowerCase() === 'max32657' ? 'm33' : 'm4'}/src/main.c"
        },
        {
          "src": "files/CMakeLists.txt",
          "dst": "${context.soc.toLowerCase() === 'max32657' ? 'm33' : 'm4'}/CMakeLists.txt"
        },
        {
          "src": "files/prj.conf",
          "dst": "${context.soc.toLowerCase() === 'max32657' ? 'm33' : 'm4'}/prj.conf"
        },
        {
          "src": "files/README.rst",
          "dst": "${context.soc.toLowerCase() === 'max32657' ? 'm33' : 'm4'}/README.rst"
        },
        {
          "src": "files/sample.yaml",
          "dst": "${context.soc.toLowerCase() === 'max32657' ? 'm33' : 'm4'}/sample.yaml"
        }
      ],
      "templates": [
        {
          "src": "templates/.cfsconfig.eta",
          "dst": ".cfs/${context.soc.toLowerCase()}-${context.package.toLowerCase()}.cfsconfig",
          "condition": "${context.soc.toLowerCase() === 'max32655'}"
        },
        {
          "src": "templates/.cfsconfig.eta",
          "dst": ".cfs/${context.soc.toLowerCase()}-${context.package.toLowerCase()}.cfsconfig",
          "condition": "${context.soc.toLowerCase() === 'max32657'}"
        },
        {
          "src": "templates/.cfsconfig.eta",
          "dst": ".cfs/${context.soc.toLowerCase()}-${context.package.toLowerCase()}.cfsconfig",
          "condition": "${context.soc.toLowerCase() === 'max32690'}"
        },
        {
          "src": "templates/workspace-name.code-workspace.eta",
          "dst": "${context.workspaceName}.code-workspace"
        },
        {
          "src": "templates/.vscode/settings.json.eta",
          "dst": "${context.soc.toLowerCase() === 'max32657' ? 'm33' : 'm4'}/.vscode/settings.json"
        }
      ]
