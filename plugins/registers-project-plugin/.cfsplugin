  "features": {
    "project": {
      "templates": [
        {
          "src": "templates/settings.json.eta",
          "dst": ".vscode/settings.json"
        },
        {
          "src": "templates/launch.json.eta",
          "dst": ".vscode/launch.json"
        },
        {
          "src": "templates/c_cpp_properties.json.eta",
          "dst": ".vscode/c_cpp_properties.json"
        },
        {
          "src": "templates/main.c.eta",
          "dst": "src/main.c"
        },
        {
          "src": "templates/CMakeLists.txt.eta",
          "dst": "CMakeLists.txt",
          "condition": "${!context.soc.toLowerCase().includes('adsp')}"
        },
        {
          "src": "templates/sharc-fx/sfx/Makefile.eta",
          "dst": "Makefile",
          "condition": "${context.coreId.toLowerCase().includes('fx')}"
        },
        {
          "src": "templates/sharc-fx/sfx/system/adi_initialize.c.eta",
          "dst": "src/system/adi_initialize.c",
          "condition": "${context.coreId.toLowerCase().includes('fx')}"
        },
        {
          "src": "templates/sharc-fx/sfx/system/adi_initialize.h.eta",
          "dst": "src/system/adi_initialize.h",
          "condition": "${context.coreId.toLowerCase().includes('fx')}"
        },
        {
          "src": "templates/sharc-fx/sfx/cfs.tasks.json.eta",
          "dst": ".vscode/tasks.json",
          "condition": "${context.coreId.toLowerCase().includes('fx')}"
        },
        {
          "src": "templates/sharc-fx/sfx/project.mk.eta",
          "dst": "project.mk",
          "condition": "${context.coreId.toLowerCase().includes('fx')}"
        }
      ],
      "files": []
    },
    "codegen": {
      "files": [],
      "templates": [
        {
          "src": "templates/memory.ld.eta",
          "dst": "memory.ld",
          "condition": "${!context.cfsconfig.Soc.toLowerCase().includes('adsp')}"
        },
        {
          "src": "templates/soc_init.c.eta",
          "dst": "soc_init.c",
          "condition": "${!context.cfsconfig.Soc.toLowerCase().includes('adsp')}"
        },
        {
          "src": "templates/soc_init.c.eta",
          "dst": "src/system/soc_init.c",
          "condition": "${context.cfsconfig.Soc.toLowerCase().includes('adsp')}"
        },
        {
          "src": "templates/tflm_lib.mk.eta",
          "dst": "sfx/tflm_lib.mk",
          "condition": "${(context.cfsconfig.Projects?.find(p => p.ProjectId == context.projectId).AIModels?.length ?? 0) > 0}"
        }
      ]
    }
  },
  "properties": {
    "project": [
      {
        "id": "ProjectName",
        "name": "Project Name",
        "category": "Project Settings",
        "default": "${context.coreId}",
        "type": "string",
        "required": true,
        "readonly": false
