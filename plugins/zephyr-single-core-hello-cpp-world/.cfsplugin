{
  "pluginName": "Zephyr Single Core Hello World C++",
  "pluginDescription": "Print \"Hello World\" to the console in C++.",
  "pluginId": "com.analog.zephyr.workspace.hellocppworld",
  "pluginVersion": "1.0.0",
  "author": "Analog Devices, Inc.",
  "firmwarePlatform": "zephyr",
  "supportedSocs": [
    {
      "name": "MAX32655",
      "dataModel": "max32655-ctbga.json",
      "board": "EvKit_V1",
      "package": "ctbga"
    },
    {
      "name": "MAX32655",
      "dataModel": "max32655-ctbga.json",
      "board": "FTHR",
      "package": "ctbga"
    },
    {
      "name": "MAX32672",
      "board": "EvKit_V1",
      "package": "tqfn40"
    },   
    {
      "name": "MAX32690",
      "dataModel": "max32690-tqfn.json",
      "board": "EvKit_V1",
      "package": "tqfn"
    },
    {
      "name": "MAX32690",
      "dataModel": "max32690-wlp.json",
      "board": "AD-APARD32690-SL",
      "package": "wlp"
    },
    {
      "name": "MAX32690",
      "dataModel": "max32690-tqfn.json",
      "board": "FTHR",
      "package": "tqfn"
    }
  ],
  "features": {
    "workspace": {
      "files": [
        {
          "src": "files/.vscode/c_cpp_properties.json",
          "dst": "${['max32657','max32658'].includes(context.soc.toLowerCase()) ? 'm33' : 'm4'}/.vscode/c_cpp_properties.json"
        },
        {
          "src": "files/src/main.cpp",
          "dst": "${['max32657','max32658'].includes(context.soc.toLowerCase()) ? 'm33' : 'm4'}/src/main.cpp"
        },
        {
          "src": "files/CMakeLists.txt",
          "dst": "${['max32657','max32658'].includes(context.soc.toLowerCase()) ? 'm33' : 'm4'}/CMakeLists.txt"
        },
        {
          "src": "files/README.rst",
          "dst": "${['max32657','max32658'].includes(context.soc.toLowerCase()) ? 'm33' : 'm4'}/README.rst"
        },
        {
          "src": "files/sample.yaml",
          "dst": "${['max32657','max32658'].includes(context.soc.toLowerCase()) ? 'm33' : 'm4'}/sample.yaml"
        },
        {
          "src": "files/gdb_toolbox/configs/fault-analyzer.json",
          "dst": ".cfs/gdb_toolbox/configs/fault-analyzer.json"
        },
        {
          "src": "files/gdb_toolbox/configs/memory-dump.json",
          "dst": ".cfs/gdb_toolbox/configs/memory-dump.json"
        },
        {
          "src": "files/gdb_toolbox/configs/register-dump.json",
          "dst": ".cfs/gdb_toolbox/configs/register-dump.json"
        },
        {
          "src": "files/gdb_toolbox/configs/thread-analyzer.json",
          "dst": ".cfs/gdb_toolbox/configs/thread-analyzer.json"
        },
        {
          "src": "files/gdb_toolbox/configs/crash-cause-zephyr.json",
          "dst": ".cfs/gdb_toolbox/configs/crash-cause-zephyr.json"
        },
        {
          "src": "files/gdb_toolbox/configs/heap-analyzer.json",
          "dst": ".cfs/gdb_toolbox/configs/heap-analyzer.json"
        },
        {
          "src": "files/gdb_toolbox/gdb/fault-analyzer-arm.gdb",
          "dst": ".cfs/gdb_toolbox/gdb/fault-analyzer-arm.gdb"
        },
        {
          "src": "files/gdb_toolbox/gdb/thread-analyzer.gdb",
          "dst": ".cfs/gdb_toolbox/gdb/thread-analyzer.gdb"
        },
        {
          "src": "files/gdb_toolbox/gdb/crash-cause-zephyr.gdb",
          "dst": ".cfs/gdb_toolbox/gdb/crash-cause-zephyr.gdb"
        },
        {
          "src": "files/gdb_toolbox/gdb/heap-analyzer.gdb",
          "dst": ".cfs/gdb_toolbox/gdb/heap-analyzer.gdb"
        },
        {
          "src": "files/.cfsdependencies",
          "dst": ".cfs"
        }
      ],
      "templates": [
        {
          "src": "templates/.cfsconfig.eta",
          "dst": ".cfs/${context.soc.toLowerCase()}-${context.package.toLowerCase()}.cfsconfig",
          "condition": "${['max32650', 'max32655', 'max32657', 'max32658', 'max32670', 'max32690', 'max78000', 'max78002'].includes(context.soc.toLowerCase())}"
        },
        {
          "src": "templates/workspace-name.code-workspace.eta",
          "dst": "${context.workspaceName}.code-workspace"
        },
        {
          "src": "templates/.vscode/settings.json.eta",
          "dst": "${['max32657','max32658'].includes(context.soc.toLowerCase()) ? 'm33' : 'm4'}/.vscode/settings.json"
        },
        {
          "src": "templates/prj.conf.eta",
          "dst": "${['max32657','max32658'].includes(context.soc.toLowerCase()) ? 'm33' : 'm4'}/prj.conf"
        },
        {
          "src": "templates/.vscode/launch.json.eta",
          "dst": "${['max32657','max32658'].includes(context.soc.toLowerCase()) ? 'm33' : 'm4'}/.vscode/launch.json"
        },
        {
          "src": "templates/ozone-project.jdebug.eta",
          "dst": "${['max32657','max32658'].includes(context.soc.toLowerCase()) ? 'm33' : 'm4'}/${['max32657','max32658'].includes(context.soc.toLowerCase()) ? 'm33' : 'm4'}.jdebug"
        }
      ]
    }
  }
}
