<%
it.getMsdkBoardName = function() {
  switch (it.board.toLowerCase()) {
    case "evkit_v1":
      return "EvKit_V1";
    case "fthr":
      if (it.soc === "MAX32690") {
        return "FTHR";
      } else if (it.soc === "MAX78000") {
        return "FTHR_RevA";
      } else if (it.soc === "MAX32650") {
        return "FTHR_APPS_A";
      } else {
        return "FTHR_Apps_P1";
      }
    case "ad-apard32690-sl":
    case "apard":
      return "APARD";
  }
};
%>