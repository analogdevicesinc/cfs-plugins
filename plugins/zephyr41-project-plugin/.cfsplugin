      {
        "id": "BuildSystem",
        "name": "Build System",
        "category": "Build Settings",
        "default": "ninja",
        "placeholder": "",
        "type": "array",
        "enum": [
          {
            "value": "ninja",
            "label": "Ninja"
          }
        ],
        "required": true
      },
      {
        "id": "ZephyrBoardName",
        "name": "Zephyr Board Name",
        "default": "",
        "placeholder": "",
        "type": "string",
        "required": true
