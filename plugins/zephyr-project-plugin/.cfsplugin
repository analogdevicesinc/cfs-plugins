  "features": {
    "project": {
      "templates": [
        {
          "src": "templates/.vscode/settings.json.eta",
          "dst": ".vscode/settings.json"
        },
        {
          "src": "templates/CMakeLists.txt.eta",
          "dst": "/"
        },
        {
          "src": "templates/prj.conf.eta",
          "dst": "prj.conf"
        },
        {
          "src": "templates/launch.json.eta",
          "dst": ".vscode/launch.json"
        },
        {
          "src": "templates/src/FastDSP/SStudioPlusConfig.json.eta",
          "dst": "src/FastDSP/SStudioPlusConfig.json",
          "condition": "${context.platformConfig.CreateSigmaStudioPlusProject}"
        },
        {
          "src": "templates/arm/ozone-project.jdebug.eta",
          "dst": "${['max32657','max32658'].includes(context.soc.toLowerCase()) ? 'm33' : 'm4'}.jdebug",
          "condition": "${['CM4', 'CM33', 'CM55'].includes(context.coreId)}"
        }
      ],
      "files": [
        {
          "src": "files/.vscode/c_cpp_properties.json",
          "dst": ".vscode/"
        },
        {
          "src": "files/README.rst",
          "dst": "/"
        },
        {
          "src": "files/src/main.c",
          "dst": "src/"
        }
      ]
    },
    "codegen": {
      "files": [],
      "templates": [
        {
          "src": "templates/board.conf.eta",
          "dst": "boards/${context.cfsconfig.Projects.find(proj => proj.ProjectId === context.projectId)?.PlatformConfig.ZephyrBoardName.split(\"/\").join(\"_\")}.conf"
        },
        {
          "src": "templates/project.overlay.eta",
          "dst": "boards/${context.cfsconfig.Projects.find(proj => proj.ProjectId === context.projectId)?.PlatformConfig.ZephyrBoardName.split(\"/\").join(\"_\")}.overlay"
        },
        {
          "src": "templates/aimodels.cmake.eta",
          "dst": "aimodels.cmake",
          "condition": "${(context.cfsconfig.Projects?.find(p => p.ProjectId == context.projectId).AIModels?.length ?? 0) > 0}"
        },
        {
          "src": "templates/dfg_config.h.eta",
          "dst": "src/dfg_config.h",
          "condition": "${(context.cfsconfig.DFG?.Streams?.length ?? 0) > 0}"
        }
      ]
    }
  },
