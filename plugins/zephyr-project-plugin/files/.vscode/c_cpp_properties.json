{
  "env": {
    "cfs.zephyr.workspace.path": ""
  },
  "configurations": [
    {
      "name": "Win32",
      "includePath": [
        "${workspaceFolder}/**",
        "${config:cfs.zephyr.sdk.path}/arm-zephyr-eabi/arm-zephyr-eabi/include",
        "${env:cfs.zephyr.workspace.path}/include/zephyr",
        "${env:cfs.zephyr.workspace.path}/include"
      ],
      "defines": ["${config:cfs.project.board}", "__GNUC__"],
      "intelliSenseMode": "gcc-arm",
      "compilerPath": "${config:cfs.zephyr.sdk.path}/arm-zephyr-eabi/bin/arm-zephyr-eabi-gcc.exe",
      "browse": {
        "path": [
          "${workspaceFolder}/**",
          "${config:cfs.zephyr.sdk.path}/arm-zephyr-eabi/arm-zephyr-eabi/include",
          "${env:cfs.zephyr.workspace.path}/include/zephyr",
          "${env:cfs.zephyr.workspace.path}/include"
        ]
      }
    },
    {
