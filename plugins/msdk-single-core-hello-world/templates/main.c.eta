<%
function ledValue() {
  switch(it.soc.toLowerCase()) {
    case "max78002":
    case "max78000":
      return "LED1";
    case "max32690":
    case "max32655":
      return "LED_RED";
    default:
      return "0";
  }
}
%>
<%
function timeOut() {
  switch(it.soc.toLowerCase()) {
    case "max32672":
      return "MXC_Delay(MXC_DELAY_MSEC(500))";
    default:
      return "500000";
  }
}
%>
