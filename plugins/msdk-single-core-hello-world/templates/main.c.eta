#include <stdio.h>
#include <stdint.h>
#include "mxc_device.h"
#include "led.h"
#include "pb.h"
#include "board.h"
#include "mxc_delay.h"

int main(void) {
    int count = 0;

    printf("Hello World!\n");

    while (1) {
        LED_On(<%= ledValue() %>);
        MXC_Delay(<%= timeOut() %>);
        LED_Off(<%= ledValue() %>);
        MXC_Delay(<%= timeOut() %>);
        printf("count = %d\n", count++);
    }
}