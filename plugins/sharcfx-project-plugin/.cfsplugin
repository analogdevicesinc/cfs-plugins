  "features": {
    "project": {
      "templates": [
        {
          "src": "templates/m33/settings.json.eta",
          "dst": ".vscode/settings.json",
          "condition": "${context.coreId.toLowerCase().includes('m33')}"
        },
        {
          "src": "templates/sfx/settings.json.eta",
          "dst": ".vscode/settings.json",
          "condition": "${context.coreId.toLowerCase().includes('fx')}"
        },
        {
          "src": "templates/m33/launch.json.eta",
          "dst": ".vscode/launch.json",
          "condition": "${context.coreId.toLowerCase().includes('m33')}"
        },
        {
          "src": "templates/sfx/launch.json.eta",
          "dst": ".vscode/launch.json",
          "condition": "${context.coreId.toLowerCase().includes('fx')}"
        },
        {
          "src": "templates/m33/c_cpp_properties.json.eta",
          "dst": ".vscode/c_cpp_properties.json",
          "condition": "${context.coreId.toLowerCase().includes('m33')}"
        },
        {
          "src": "templates/sfx/c_cpp_properties.json.eta",
          "dst": ".vscode/c_cpp_properties.json",
          "condition": "${context.coreId.toLowerCase().includes('fx')}"
        },
        {
          "src": "templates/m33/cfs.tasks.json.eta",
          "dst": ".vscode/cfs.tasks.json",
          "condition": "${context.coreId.toLowerCase().includes('m33')}"
        },
        {
          "src": "templates/sfx/cfs.tasks.json.eta",
          "dst": ".vscode/cfs.tasks.json",
          "condition": "${context.coreId.toLowerCase().includes('fx')}"
        },
        {
          "src": "templates/m33/main.c.eta",
          "dst": "src/main.c",
          "condition": "${context.coreId.toLowerCase().includes('m33')}"
        },
        {
          "src": "templates/sfx/main.c.eta",
          "dst": "src/main.c",
          "condition": "${context.coreId.toLowerCase().includes('fx')}"
        },
        {
          "src": "templates/m33/Makefile.eta",
          "dst": "Makefile",
          "condition": "${context.coreId.toLowerCase().includes('m33')}"
        },
        {
          "src": "templates/sfx/Makefile.eta",
          "dst": "Makefile",
          "condition": "${context.coreId.toLowerCase().includes('fx')}"
        },
        {
          "src": "templates/m33/project.mk.eta",
          "dst": "project.mk",
          "condition": "${context.coreId.toLowerCase().includes('m33')}"
        },
        {
          "src": "templates/sfx/project.mk.eta",
          "dst": "project.mk",
          "condition": "${context.coreId.toLowerCase().includes('fx')}"
        },
        {
          "src": "templates/system/adi_initialize.c.eta",
          "dst": "src/system/adi_initialize.c",
          "condition": "${context.coreId.toLowerCase().includes('fx')}"
        },
        {
          "src": "templates/system/adi_initialize.h.eta",
          "dst": "src/system/adi_initialize.h",
          "condition": "${context.coreId.toLowerCase().includes('fx')}"
        },
        {
          "src": "templates/ozone-project.jdebug.eta",
          "dst": "m33.jdebug",
          "condition": "${context.coreId.toLowerCase().includes('m33')}"
        },
        {
          "src": "templates/m33/ADSP-SC83x-1MB.ld.eta",
          "dst": "ADSP-SC83x-1MB.ld",
          "condition": "${context.soc.toLowerCase().includes('sc834') && context.coreId.toLowerCase().includes('m33')}"
        },
        {
          "src": "templates/m33/ADSP-SC83x-2MB.ld.eta",
          "dst": "ADSP-SC83x-2MB.ld",
          "condition": "${context.soc.toLowerCase().includes('sc835') && context.coreId.toLowerCase().includes('m33')}"
        }
      ],
      "files": [
        {
          "src": "files/.cfsdependencies",
          "dst": ".cfs"
        }
      ]
    },
    "codegen": {
      "templates": [
        {
          "src": "templates/aimodels_sfx.mk.eta",
          "dst": "aimodels.mk",
          "condition": "${((context.cfsconfig.Projects?.find(p => p.ProjectId == context.projectId).AIModels?.length ?? 0) > 0) && context.coreId.toLowerCase().includes('fx')}"
        },
        {
          "src": "templates/aimodels_m33.mk.eta",
          "dst": "aimodels.mk",
          "condition": "${((context.cfsconfig.Projects?.find(p => p.ProjectId == context.projectId).AIModels?.length ?? 0) > 0) && context.coreId.toLowerCase().includes('m33')}"
        },
        {
          "src": "templates/soc_init.c.eta",
          "dst": "src/system/soc_init.c",
          "condition": "${context.coreId.toLowerCase().includes('m33')}"
        },
        {
          "src": "templates/soc_init.c.eta",
          "dst": "src/system/soc_init.c",
          "condition": "${context.coreId.toLowerCase().includes('fx')}"
        }
      ],
      "files": []
    }
  },
  "properties": {
    "project": [
      {
        "id": "ProjectName",
        "name": "Project Name",
        "category": "Project Settings",
        "default": "${context.coreId}",
        "type": "string",
        "required": true,
        "readonly": false
      }
    ],
    "peripheral": {}
  }
}
